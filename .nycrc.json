{
    "include": [
        "src"
    ],
    "exclude": [
        "src/decorators.js"
    ],
    "reporter": [
        "html",
        "lcovonly"
    ],
    "require": [
        "@babel/register",
        "@babel/polyfill"
    ],
    "report-dir": "./coverage",
    "temp-dir": "./coverage",
    "lines": 90,
    "statements": 90,
    "functions": 80,
    "branches": 80,
    "check-coverage": true
}