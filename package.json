{
  "name": "myrmidon",
  "version": "1.3.4",
  "description": "package myrmidon provides list of common utilties for comfortable work with js projects.",
  "main": "./lib",
  "scripts": {
    "build": "babel src --out-dir ./lib",
    "prepare": "npm run build",
    "push:coveralls": "cat ./coverage/lcov.info | coveralls",
    "push:release": "semantic-release",
    "test:security": "npm audit",
    "test:lint": "eslint src tests tools bin",
    "test:mocha": "mocha  --config .mocharc.json 'tests/**/*.test.js'",
    "save-examples": "SAVE_EXAMPLES=tmp/examples.json mocha  --config .mocharc.json 'tests/**/*.test.js' --grep '@example'",
    "test:coverage": "MODE=test nyc mocha --config .mocharc.json 'tests/**/*.test.js'",
    "test": "npm run test:lint && npm run test:security || npm run test:coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pustovitDmytro/myrmidon"
  },
  "author": "Dmytro Pustovit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pustovitDmytro/myrmidon/issues"
  },
  "homepage": "https://myrmidonjs.readthedocs.io",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "chai": "^4.2.0",
    "chance": "^1.0.18",
    "cls-hooked": "^4.2.2",
    "conventional-changelog-eslint": "^3.0.1",
    "coveralls": "^3.0.3",
    "docopt": "^0.6.2",
    "documentation": "^13.0.0",
    "escodegen": "^1.14.1",
    "eslint": "^6.8.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.1",
    "esprima": "^4.0.1",
    "fs-extra": "^8.1.0",
    "handlebars": "^4.3.1",
    "mdinclude": "^0.7.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "remark": "^11.0.1",
    "remark-preset-lint-recommended": "^3.0.3",
    "remark-toc": "^6.0.0",
    "semantic-release": "^17.0.7",
    "swag": "^0.7.0",
    "uuid": "^3.3.2"
  }
}
